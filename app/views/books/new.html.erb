<h1>Add new book</h1>

<%= form_with model: @book do |f| %>
  <p><%= f.label :title %>:
    <%= f.text_field :'title' %></p>

  <p><%= f.label :author %>:
    <%= f.text_field :'author' %></p>

  <p><%= f.label :isbn %>:
    <%= f.text_field :'isbn' %></p>

  <%= submit_tag "Create" %>
<% end -%>

<%= link_to 'Back', {:action => 'index'} %>
